{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ES2020",
		"lib": [ "ES2020", "DOM" ],
		"outDir": "./wwwroot/js",
		"rootDir": "./TypeScript",
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"moduleResolution": "node"
	},
	"include": [ "TypeScript/**/*" ],
	"exclude": [ "node_modules" ]
}
